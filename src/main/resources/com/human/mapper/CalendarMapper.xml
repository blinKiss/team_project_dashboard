<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.CalendarMapper">
    <insert id="saveAttendance" parameterType="com.example.model.Attendance">
        INSERT INTO calendar (user_id, attendance, start_date, end_date)
        VALUES (#{userId}, 1, #{startDate}, #{endDate})
    </insert>
</mapper>
